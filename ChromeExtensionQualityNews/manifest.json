{
    "name": "Qualnewser",
    "version": "0.1",
    "manifest_version": 2,
    "description": "A sidebar with a high-quality suggestions on the article you're watching",
    "icons": { "128": "qualitynews.png" },
    "converted_from_user_script": true,
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
     {
        "matches": ["*://*.co.uk/*"],
        "js": ["jquery.min.js", "content.user.js"]
      }
    ]
}